<div class="page-body-infoB">
    <div class="infoBContent">
        {{#infoB}}
            <div class="header">
                {{#header}}
                    <div class="title">{{title}}</div>
                    <div class="list">
                        {{#each list}}
                            {{#each .}}
                                <a href="{{.}}">{{@key}}</a>
                            {{/each}}
                        {{/each}}
                    </div>
                {{/header}}
            </div>
            <div class="body">
                <div class="infoContent">
                    {{#main}}
                        {{#company}}
                            <a class="company" href="{{companyUrl}}">
                                <img src="{{companyImg}}"/>
                            </a>
                        {{/company}}
                        {{#each shopList}}
                            {{#isShow @index 6}}
                                <a class="shop" href="{{shopUrl}}">
                                    <div class="shopImg">
                                        <img src="{{shopImg}}"/>
                                    </div>
                                    <div class="text">
                                        <span class="title">
                                            {{shopTitle}}
                                        </span>
                                        <span class="price">{{parsefloat shopPrice}}</span>
                                    </div>
                                </a>
                            {{/isShow}}
                        {{/each}}
                    {{/main}}
                </div>
            </div>
        {{/infoB}}
    </div>
</div>