<div class="page-company-content" memberId="{{RESPONSE.memberId}}">
    <div class="maxWidth">
        <div class="companyLogo">
            <img src="{{getImgUrl RESPONSE.topPicPath}}">
        </div>
    </div>
    <div class="companyHeader">
        <div class="maxWidth">
            <span class="companyName">{{RESPONSE.memberName}}</span>
        </div>
    </div>
    <div class="maxWidth">
        <div class="companyInfo">
            <div class="companyContentLeft">
                <div class="infoTitle"> 企业信息 </div>
                <div class="infoDescription">
                    <span class="one">经营者：</span><span class="two">{{RESPONSE.operator}}</span>
                    <span class="one">电话：</span><span class="two">{{RESPONSE.mobile}}</span>
                    <span class="one">手机：</span><span class="two">{{RESPONSE.phone}}</span>
                    <span class="one">邮箱：</span><span class="two">{{RESPONSE.email}}</span>
                    <span class="one">QQ号：</span><span class="two">{{RESPONSE.qq}}</span>
                    <span class="one">商家信用:</span><span class="two">{{RESPONSE.reputation}}</span>
                    <span class="one">经营类目:</span>
                    <span class="two">
                        {{#each RESPONSE.industryList}}
                            {{convertStr industryName "、"}}、
                        {{/each}}
                    </span>
                    <span class="one">商铺地址:</span>
                    <span class="two">
                        {{#RESPONSE}}
                            {{province}}{{city}}{{area}}{{address}}
                        {{/RESPONSE}}
                    </span>
                    <span class="one">主营商品:</span>
                    <span class="two">
                        {{RESPONSE.product}}
                    </span>
                    <span class="one" style="margin: 6px 0 10px 0px;">
                        <a href="javascript:void(0)" class="licenseBtn" licensePath="{{licensePath}}">营业执照</a>
                    </span>
                    <span class="one pull-right" style="margin: 6px 0 10px 0px;">
                        <a href="javascript:void(0)" class="collectionBtn">收藏店铺</a>
                    </span>
                </div>
                <div class="pictrueTitle"> 企业相册 </div>
                <div class="pictrueDescription">
                    {{#each RESPONSE.queryShop.memberGoodCommodityList}}
                        <a href="/app/webpage/shop.html?memberId={{memberId}}&commodityId={{commodityId}}" target="_blank">
                            <img src="{{getImgUrl picture1 null "default_search.jpg"}}">
                            <div class="price">{{format_Money price}}</div>
                        </a>
                    {{/each}}
                </div>
            </div>
            <div class="companyContentRight">
                <div class="companyImg">
                    {{#each RESPONSE.fileList}}
                        <img src="{{getImgUrl finalName suffix "default_companyLogo.jpg"}}" index={{@index}} style="z-index:{{zIndexDesc @index 100}}">
                    {{/each}}
                    {{#noImg RESPONSE.fileList}}
                        <img src="/app/images/default_companyLogo.jpg">
                    {{/noImg}}
                    {{#length RESPONSE.fileList}}
                        <span class="carousel_list">
                            {{#each RESPONSE.fileList}}
                                <i class="carousel_i {{addElementClass @index 0 'showing'}}" index="{{@index}}"></i>
                            {{/each}}
                        </span>
                        <a class="carousel_btn prev"><</a>
                        <a class="carousel_btn next">></a>
                    {{/length}}
                </div>
                {{#RESPONSE.aboutUs}}
                    <div class="companyDescription">
                        {{RESPONSE.aboutUs}}
                    </div>
                {{/RESPONSE.aboutUs}}
                <div class="companyShops">
                    {{#isExist RESPONSE.queryShop.commodityCategory2}}
                        <div class="shopClassify commodityCategory2">
                            <div>主营类目:</div>
                            <div>
                                {{#each RESPONSE.queryShop.commodityCategory2}}
                                    <a href="javascript:void(0)" class="commodityCategory2Item">{{.}}</a>
                                {{/each}}
                            </div>
                        </div>
                    {{/isExist}}
                    {{#isExist RESPONSE.queryShop.commodityCategory3}}
                        <div class="shopClassify commodityCategory3">
                            <div>类型分类:</div>
                            <div>
                                {{#each RESPONSE.queryShop.commodityCategory3}}
                                    <a href="javascript:void(0)" class="commodityCategory3Item">{{.}}</a>
                                {{/each}}
                            </div>
                        </div>
                    {{/isExist}}
                </div>
                <div class="companySort">
                    <a href="javascript:void(0)" class="active">综合排序</a>
                    <a href="javascript:void(0)" class="filterHot">热销
                        <i class="uFont font-base_directionDown" style="vertical-align:-2px;"></i>
                    </a>
                    <a href="javascript:void(0)" class="filterPrice">价格
                        <i class="uFont font-base_directionDown" style="vertical-align:-2px;"></i>
                    </a>
                </div>
                {{#isExist RESPONSE.queryShop.commoditySearchList}}
                    <div class="companyShow">
                        <ul>
                            {{#each RESPONSE.queryShop.commoditySearchList}}
                                <li>
                                    <a href="/app/webpage/shop.html?memberId={{memberId}}&commodityId={{commodityId}}" target="_blank">
                                        <div class="imgShow">
                                            <img src="{{getImgUrl picture1 null "default_search.jpg"}}">
                                        </div>
                                        <div class="description">
                                            <abbr title="{{commodityName}}">
                                                {{commodityName}}
                                            </abbr>
                                        </div>
                                        <div class="price">
                                            {{format_Money price}}
                                        </div>
                                    </a>
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                    <div id="load-pagePagination"></div>
                {{/isExist}}
                {{^isExist RESPONSE.queryShop.commoditySearchList}}
                    <div class="nothingError">
                        <span class="description"><i class="fa fa-warning fa-fw"></i>抱歉没有找到</span>
                        <span class="description">相关商品!</span>
                    </div>
                {{/isExist}}
            </div>
        </div>
    </div>
</div>